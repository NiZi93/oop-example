package interfaces;

/**
 *
 * @author User
 */
public class Singer implements Speaker {
    @Override
    
    public void speak()
    {
        System.out.println("speak2");
    }
    public void sing()
    {
        System.out.println("sing");
    }
    public Singer(String name)
    {
        System.out.println(name+ " singing");
    }
}
